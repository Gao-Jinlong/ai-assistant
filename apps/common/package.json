{
  "name": "@ai-assistant/common",
  "version": "0.0.1",
  "description": "Common utilities and types shared between server and web",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "source": "./src/index.ts"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js",
      "source": "./src/types/index.ts"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.js",
      "source": "./src/utils/index.ts"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.mjs",
      "require": "./dist/constants.js",
      "source": "./src/constants/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "zod": "^3.24.2",
    "dayjs": "^1.11.13",
    "nanoid": "^3.3.8"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.22.0",
    "tsup": "^8.3.5",
    "typescript": "^5.1.3",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "access": "restricted"
  }
}