{
  "name": "@ai-assistant/server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "npm-run-all2 --sequential prisma:migrate:dev prisma:generate start:debug:no-clear",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:pull": "prisma db pull",
    "prisma:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "prisma:migrate:dev": "cross-env NODE_ENV=development prisma migrate deploy",
    "prisma:migrate:prod": "cross-env NODE_ENV=production prisma migrate deploy",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "cross-env DEBUG=* NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:debug:no-clear": "cross-env NODE_ENV=development nest start --debug --watch --preserveWatchOutput",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "start:repl": "nest start --entryFile repl",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:setup:dev": "cross-env NODE_ENV=development tsx scripts/setup-database.ts",
    "db:setup:prod": "cross-env NODE_ENV=production tsx scripts/setup-database.ts",
    "db:setup:test": "cross-env NODE_ENV=test tsx scripts/setup-database.ts"
  },
  "dependencies": {
    "@ai-assistant/common": "workspace:*",
    "@aws-sdk/client-s3": "^3.777.0",
    "@keyv/redis": "^5.1.1",
    "@langchain/community": "^1.0.0",
    "@langchain/core": "^1.0.1",
    "@langchain/langgraph": "^1.0.0",
    "@langchain/openai": "^1.0.0",
    "@langchain/tavily": "^1.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.7",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.7",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^11.1.10",
    "@nestjs/platform-express": "^11.1.7",
    "@nestjs/swagger": "^11.2.1",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "@prisma/client-runtime-utils": "^7.0.0",
    "@types/fs-extra": "^11.0.4",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^7.1.1",
    "cache-manager-memory-store": "^1.1.0",
    "cacheable": "^1.10.4",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.3",
    "es-toolkit": "^1.32.0",
    "fs-extra": "^11.3.0",
    "install": "^0.13.0",
    "ioredis": "^5.8.2",
    "kafkajs": "^2.2.4",
    "keyv": "^5.5.0",
    "langchain": "^0.3.19",
    "nanoid": "^3.3.8",
    "nest-winston": "^1.10.2",
    "nestjs-cls": "^6.0.1",
    "nestjs-zod": "^5.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.22.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.19.23",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.0.0",
    "prisma": "^7.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.27.0",
    "webpack": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}