{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    // "emitDecoratorMetadata": true,
    // "experimentalDecorators": true,
    // "target": "ES2021",
    "baseUrl": "./",
    // "incremental": true,
    // "skipLibCheck": true,
    // "strictNullChecks": true,
    // "noImplicitAny": true,
    // "strictBindCallApply": true,
    // "forceConsistentCasingInFileNames": true,
